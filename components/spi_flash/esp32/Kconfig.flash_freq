choice ESPTOOLPY_FLASHFREQ
    prompt "Flash SPI speed"
    default ESPTOOLPY_FLASHFREQ_40M
    config ESPTOOLPY_FLASHFREQ_80M
        bool "80 MHz"
    config ESPTOOLPY_FLASHFREQ_40M
        bool "40 MHz"
    config ESPTOOLPY_FLASHFREQ_26M
        bool "26 MHz"
    config ESPTOOLPY_FLASHFREQ_20M
        bool "20 MHz"
endchoice
