[mapping:hal]
archive: libhal.a
entries:
    if APP_BUILD_TYPE_PURE_RAM_APP = n:
        mmu_hal (noflash)
        if IDF_TARGET_ESP32 = y:
            cache_hal_esp32 (noflash)
        else:
            cache_hal (noflash)
    if SOC_SYSTIMER_SUPPORTED = y && HAL_SYSTIMER_USE_ROM_IMPL = n:
        systimer_hal (noflash)
