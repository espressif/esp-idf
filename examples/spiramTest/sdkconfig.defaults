
CONFIG_BOOTLOADER_LOG_LEVEL_NONE=y

CONFIG_COMPILER_STACK_CHECK_MODE_ALL=y
CONFIG_COMPILER_OPTIMIZATION_ASSERTIONS_ENABLE=y
CONFIG_COMPILER_OPTIMIZATION_CHECKS_SILENT=n
CONFIG_COMPILER_WARN_WRITE_STRINGS=y

CONFIG_HEAP_TASK_TRACKING=y
CONFIG_HEAP_TRACING_STANDALONE=y
CONFIG_HEAP_POISONING_COMPREHENSIVE=y

CONFIG_FREERTOS_WATCHPOINT_END_OF_STACK=y
CONFIG_FREERTOS_USE_STATS_FORMATTING_FUNCTIONS=y
CONFIG_FREERTOS_USE_TRACE_FACILITY=y
CONFIG_FREERTOS_GENERATE_RUN_TIME_STATS=y
CONFIG_FREERTOS_CHECK_STACKOVERFLOW_CANARY=y
CONFIG_FREERTOS_WATCHPOINT_END_OF_STACK=y
CONFIG_FREERTOS_INTERRUPT_BACKTRACE=y
CONFIG_FREERTOS_ENABLE_TASK_SNAPSHOT=y
CONFIG_FREERTOS_CHECK_MUTEX_GIVEN_BY_OWNER=y

CONFIG_LOG_DEFAULT_LEVEL_INFO=y
CONFIG_LOG_MAXIMUM_LEVEL_VERBOSE=y


CONFIG_ESP_COREDUMP_ENABLE_TO_UART=y
CONFIG_ESP_TASK_WDT=n
CONFIG_ESP_DEBUG_STUBS_ENABLE=y



#do not modify eFuse permanently
CONFIG_EFUSE_VIRTUAL=y
CONFIG_EFUSE_VIRTUAL_KEEP_IN_FLASH=y

CONFIG_BOOT_ROM_LOG_ALWAYS_ON=y


CONFIG_ESP32S2_SPIRAM_SUPPORT=y
CONFIG_ESP32S2_DEFAULT_CPU_FREQ_240=y
CONFIG_ESP32S2_INSTRUCTION_CACHE_16KB=y
CONFIG_SPIRAM_SPEED_80M=y

